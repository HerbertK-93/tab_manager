{
    "manifest_version": 3,
    "name": "Tab Manager",
    "version": "1.0",
    "description": "Manage and organize your open tabs efficiently.",
    "permissions": [
      "tabs",
      "storage"
    ],
    "action": {
      "default_popup": "tabs.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  